{
  "name": "hooks-typescript-demo-quiz",
  "version": "0.1.0",
  "description": "A simple quiz created with React hooks/typescript. Filters out HTML entities and displays UK/US spellings.",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^10.4.9",
    "@testing-library/user-event": "^12.1.3",
    "@types/jest": "^26.0.12",
    "@types/node": "^14.6.2",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.2",
    "framer-motion": "^2.6.5",
    "next": "^11.1.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.3",
    "styled-components": "^5.1.1",
    "typescript": "~4.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mconnor/hooks-ts-quiz"
  },
  "scripts": {
    "deploy": "now $(awk '!/^#|^\\s*$/ { printf(\"-e %s \", $0)}' .env)",
    "dev": "BROWSER=none react-scripts  start --port $PORT",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "server": {
    "nodePort": 8080
  },
  "devDependencies": {
    "@now/node": "^1.8.1"
  },
  "author": "Mike Connor <mike@cloudswing.info> (https://github.com/mconnor/)",
  "license": "ISC"
}
