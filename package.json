{
  "name": "hooks-typescript-demo-quiz",
  "version": "0.1.0",
  "private": false,
  "description": "A simple quiz created with React hooks/typescript. Filters out HTML entities and displays UK/US spellings.",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/styled-components": "^5.0.1",
    "framer-motion": "^1.9.1",
    "next": "^9.3.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-scripts": "3.4.0",
    "styled-components": "^5.0.1",
    "typescript": "~3.7.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mconnor/hooks-ts-quiz"
  },
  "scripts": {
    "deploy": "now $(awk '!/^#|^\\s*$/ { printf(\"-e %s \", $0)}' .env)",
    "dev": "BROWSER=none react-scripts  start --port $PORT",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@now/node": "^1.4.1"
  },
  "author": "Mike Connor <mike@cloudswing.info> (https://github.com/mconnor/)",
